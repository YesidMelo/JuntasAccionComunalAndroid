<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="16dp">

    <TextView
        android:id="@+id/textView_detalleEnJac_titulo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/detalle_en_jac"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout_detalleAfiliadoEnJac_formulario"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_detalleEnJac_titulo">

        <TextView
            android:id="@+id/textView_detalleAfiliadoEnJac_tituloComite"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/comite"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Spinner
            android:id="@+id/spinner_detalleAfiliadoEnJAC_comite"
            android:layout_width="0dp"
            android:layout_height="48dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_detalleAfiliadoEnJac_tituloComite"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/textView_detalleAfiliadoEnJac_estadoEstadoEnJunta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/estado_actual_afiliacion"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/spinner_detalleAfiliadoEnJAC_comite" />

        <Spinner
            android:id="@+id/spinner_detalleAfiliadoEnJAC_estadoAfiliadoEnJAc"
            android:layout_width="0dp"
            android:layout_height="48dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_detalleAfiliadoEnJac_estadoEstadoEnJunta"
            tools:ignore="SpeakableTextPresentCheck" />

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout_detalleAfiliadoenJAc_observaciones"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="1dp"
            app:layout_constraintDimensionRatio="2:1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/spinner_detalleAfiliadoEnJAC_estadoAfiliadoEnJAc">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/editText_detalleAfiliadoenJAc_observaciones"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:hint="@string/observaciones" />
        </com.google.android.material.textfield.TextInputLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>